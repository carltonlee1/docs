{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Pathfinder API",
  "description": "Intelligent LLM routing service with privacy-first design",
  "theme": "dark",
  "colors": {
    "primary": "#0066FF",
    "light": "#4D99FF",
    "dark": "#0052CC",
    "background": {
      "dark": "#0A0E27"
    }
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://naivana.io"
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://naivana.io/dashboard",
        "icon": "chart-line"
      },
      {
        "label": "Status",
        "href": "https://status.naivana.io",
        "icon": "circle-check"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://naivana.io/api-keys"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/naivana-internal/pathfinder",
      "x": "https://x.com/naivanaai"
    },
    "links": [
      {
        "header": "Resources",
        "items": [
          {
            "label": "Dashboard",
            "href": "https://naivana.io/dashboard"
          },
          {
            "label": "Blog",
            "href": "https://naivana.io/blog"
          }
        ]
      },
      {
        "header": "Support",
        "items": [
          {
            "label": "Support Email",
            "href": "mailto:support@naivana.io"
          },
          {
            "label": "GitHub Issues",
            "href": "https://github.com/naivana-internal/pathfinder/issues"
          }
        ]
      }
    ]
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "pathfinder/getting-started/introduction",
              "pathfinder/getting-started/quickstart",
              "pathfinder/getting-started/authentication",
              "pathfinder/getting-started/setup-credentials"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "pathfinder/core-concepts/routing-decisions",
              "pathfinder/core-concepts/privacy-first-design",
              "pathfinder/core-concepts/multi-tenancy",
              "pathfinder/core-concepts/model-providers"
            ]
          },
          {
            "group": "Integration Guides",
            "pages": [
              "pathfinder/guides/python-integration",
              "pathfinder/guides/nodejs-integration",
              "pathfinder/guides/curl-examples",
              "pathfinder/guides/cost-optimization",
              "pathfinder/guides/error-handling"
            ]
          },
          {
            "group": "Troubleshooting",
            "pages": [
              "pathfinder/support/troubleshooting",
              "pathfinder/support/faq"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Endpoints",
            "pages": [
              "pathfinder/endpoints/index",
              "pathfinder/endpoints/routing-decision",
              "pathfinder/endpoints/get-decision"
            ]
          },
          {
            "group": "Schemas",
            "pages": [
              "pathfinder/schemas/request-schemas",
              "pathfinder/schemas/response-schemas",
              "pathfinder/schemas/error-schemas"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "pathfinder/api-reference/rest-api",
              "pathfinder/api-reference/grpc-api",
              "pathfinder/api-reference/models-list"
            ]
          }
        ]
      }
    ]
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "integrations": {
    "ga4": {
      "measurementId": "G-XXXXXXX"
    }
  }
}
